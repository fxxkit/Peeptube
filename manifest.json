{
  "manifest_version": 2,

  "name": "PeepTube",
  "description": "Peeping on youtube search results",
  "version": "1.0",
   "background": {
      "persistent":false,
      "scripts": ["background.js","underscore-min.js"]
    }, 
  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*"],
      "css": ["compass/stylesheets/myStyles.css","bootstrap.css"],
      "js": ["jquery-2.1.0.min.js","bootstrap.min.js","myScript.js"]
    }
  ],
  "permissions": ["tabs", "http://www.youtube.com/*","webNavigation"]
}
